{
    "compilerOptions": {
      "target": "ES2018",
      "module": "ESNext",
      "moduleResolution": "Node",
      "lib": [
        "ESNext",
        "ESNext.AsyncIterable",
        "DOM"
      ],
      "esModuleInterop": true,
      "allowJs": true,
      "sourceMap": true,
      "strict": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
        "~/*": [
          "./*"
        ],
        "@/*": [
          "./*"
        ]
      },
      "types": [
        "node",
        "@types/node",
        "@nuxt/types",
        "@nuxtjs/auth-next",
        "@types/three",
        "buefy/types",
        "@types/spotify-web-api-node",
        "@types/animejs",
      ]
    },
    "exclude": [
      "node_modules"
    ]
  }